// Input:3
	vec2 n_out3p0 = SCREEN_UV;


// Input:24
	vec2 n_out24p0 = SCREEN_PIXEL_SIZE;
	float n_out24p1 = n_out24p0.r;
	float n_out24p2 = n_out24p0.g;


// FloatOp:28
